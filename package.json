{
	"name": "bed.money",
	"version": "0.1.0",
	"description": "Stories of stuggle and ways that people interact with money in different parts of the world",
	"main": "index.html",
	"scripts": {
		"start": "gatsby develop",
		"build": "gatsby build",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"type-check": "tsc",
		"lint": "prettier --write .",
		"format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/peakshift/bed.money.git"
	},
	"keywords": [
		"bed",
		"money",
		"mattress",
		"money",
		"user",
		"research",
		"user",
		"interviews",
		"use",
		"cases",
		"bitcoin"
	],
	"author": "Johns Beharry <johns@peakshift.com>",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/peakshift/bed.money/issues"
	},
	"homepage": "https://github.com/peakshift/bed.money#readme",
	"husky": {
		"hooks": {
			"prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
			"pre-commit": "lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"./src/**/*.{ts,js,jsx,tsx}": [
			"yarn lint --fix",
			"yarn format"
		]
	},
	"dependencies": {
		"@mdx-js/mdx": "^1.6.14",
		"@mdx-js/react": "^1.6.14",
		"gatsby": "^2.22.5",
		"gatsby-plugin-mdx": "^1.2.29",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"tsconfig-paths-webpack-plugin": "^3.2.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^8.3.5",
		"@commitlint/config-conventional": "^8.3.4",
		"@types/node": "^13.11.0",
		"@types/react": "^16.9.32",
		"@types/react-dom": "^16.9.6",
		"@typescript-eslint/eslint-plugin": "^2.27.0",
		"@typescript-eslint/parser": "^2.27.0",
		"babel-plugin-root-import": "^6.5.0",
		"commitizen": "^4.0.4",
		"cz-conventional-changelog": "^3.1.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.1",
		"eslint-import-resolver-root-import": "^1.0.4",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react": "^7.19.0",
		"eslint-plugin-react-hooks": "^3.0.0",
		"husky": "^4.2.3",
		"lint-staged": "^10.1.2",
		"prettier": "^2.0.5",
		"typescript": "^3.8.3"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
